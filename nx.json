{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "pubspec.yaml": "*",
    "analysis_options.yaml": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "format",
          "analyze"
        ],
        "runtimeCacheInputs": ["flutter --version || dart --version"],
        "accessToken": "NzRlZjYyYTctMTA0Ny00NTAyLWI1NzctNjIzMGRiNWI1Yzk3fHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "plugins": ["@nx-dart/nx-dart"],
  "cli": {
    "defaultCollection": "@nx-dart/nx-dart"
  }
}
